{
  "entrypoint": "index.html",
  "shell": "src/kal-app.html",
  "fragments": [
    "src/kal-home-page.html",
    "src/kal-game-page.html",
    "src/kal-news-page.html",
    "src/kal-view404-page.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
